    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch setup.py [Debug]",
            "type": "python",
            "request": "launch",
            "program": "setup.py",
            "args": ["build", "--debug"],
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "Launch setup.py [Debug 2]",
            "type": "python",
            "request": "launch",
            "module": "build",
            "justMyCode": false,
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "Launch setup.py [Release]",
            "type": "python",
            "request": "launch",
            "program": "setup.py",
            "args": ["build"],
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "Debug target_test [Debug]",
            "type": "python",
            "request": "launch",
            "program": "tests/target_test.py",
            "cwd": "${workspaceFolder}",
            //"python": "python_d"
        },
        {
            "name": "Debug main test [Debug]",
            "type": "python",
            "request": "launch",
            "program": "tests/main.py",
            "cwd": "${workspaceFolder}",
            "args": ["-X dev"],
            //"python": "${userHome}/cpython/debug/python"
        },
    ]
}